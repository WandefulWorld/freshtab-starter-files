{
    "manifest_version": 2,
    "name": "ourExtension",
    "version": "1.0.0",
    "description": "Experience ourExtension every time you open a new tab.",
    "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": ["storage", "unlimitedStorage"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": true
    },
    "minimum_chrome_version": "60"
  }